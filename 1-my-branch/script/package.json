{
  "name": "temporal-state",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/initial.ts",
    "start.watch": "nodemon src/worker.ts",
    "workflow.signal": "ts-node src/signal-workflow.ts",
